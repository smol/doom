{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "commonjs",
    "noResolve": false,
    "target": "es6",
    "removeComments": true,
    "declaration": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "outDir": "../.build/",
    "typeRoots": ["../.build/", "./node_modules/@types/"],
    "baseUrl": ".",
    "paths": {
      "engine": ["../../.build/engine"],
      "wad": ["../../.build/wad"],
      "react": ["node_modules/react"],
      "react-dom": ["node_modules/react-dom"]
    },
    "types": ["wad", "engine", "react", "react-dom", "node", "webpack-env"],
    "lib": ["es5", "dom", "es2015.iterable"]
  },
  "include": ["./**/*"],
  "exclude": ["**/index.d.ts", "**/node_modules/*"]
}
