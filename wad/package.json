{
  "name": "wad",
  "version": "0.1.0",
  "description": "Wad is a Typescript library to read Wad files from DoomEngine",
  "scripts": {
    "test": "ava-ts tests --verbose",
    "test:watch": "npm run test -- --watch",
    "build": "webpack"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "keywords": [
    "wad",
    "doom",
    "binary",
    "typescript"
  ],
  "author": "Smol",
  "license": "LGPL-3.0",
  "devDependencies": {
    "dts-bundle-webpack": "^1.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "webpack-cli": "^4.9.0"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "source": [
      "src/*.ts"
    ],
    "concurrency": 5,
    "failFast": true,
    "modules": false,
    "failWithoutAssertions": false,
    "powerAssert": false,
    "require": [
      "ts-node/register"
    ]
  }
}
